local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("NOX Hub", "BloodTheme")

local Tab = Window:NewTab("Main")
local MainSection = Tab:NewSection("Main")

MainSection:NewSlider("Jump Power", "Pulo do Player", 500, 50, function(v)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = v
end)

MainSection:NewSlider("Walk Speed", "Velocidade do Player", 500, 16, function(v)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
end)

MainSection:NewButton("God Mod", "Modo DEUSS!!! Aperte F9 para funcionar", function(v)
    for i = 0,50 do
        print("Para de usar Script Vagabundo!")
    end
end)

local Tab = Window:NewTab("Op")
local OpSection = Tab:NewSection("Op")


OpSection:NewButton("PARA DE USR SCRIPT LIXO!!", "PARA DE USA SCRIPT SEU LIXO!!", function()
    for i = 0,999 do
        print("PARA DE USAR SCRIPT SEU LIXO!!")
    end
